    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>SolicitudPrestamoPersonal</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@id">
            <Value>=Parameters!id.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select 
p.apellido,p.nombres,p.NroDocumento,
c.altura as AlturaPersona,
upper(c.Domicilio) as DomicilioPersona,
loc.Descripcion as Localidad
,upper(pr.Descripcion) as Provincia
,u.Mail as Correo
,c.Telefono as TelefonoPersona
,c.Celular as CelularPersona
,org.Descripcion as Organismo
,org.domicilio as OrganismoDomicilio
,org.telefono as OrganismoTelefono
,upper(pro.Descripcion) as OrganismoProvincia
,lc.descripcion as OrganismoLocalidad
,c.CBU
,pre.capital
,pre.CantidadCuotas
,pre.MontoCuota
,pre.MontoCuotaEnLetras
,pre.CFT
,pre.FechaPrimerVencimiento
,pre.Pagare
,pre.PagareEnLetras
,i.Nombre as Inversor
,i.FirmaReporte
,i.Logo
,pre.TotalCapitalInversor
,pre.CapitalInversorEnLetras
,pre.FechaPresentacionDeInversor
,Pre.FirmaOlografica
,Pre.FirmaOlograficaConfirmacion
,Pre.FechaSolicitado
, t1.Tasa AS TEA, t2.Tasa AS TNA, t3.Tasa AS CFTTotal, t4.Tasa As CFTConImpuesto, t5.Tasa AS TEM 
from Personas p inner join  AspNetUsers u on p.id=u.PersonasId
inner join TiposPersonas as tpers on tpers.Id=p.TipoPersonaId
inner join Organismos as org on org.Id=tpers.OrganismoId
left join Localidad as lc on org.localidadid=lc.id
left join Provincia as pro on pro.Id=org.ProvinciaId
inner join Clientes as c on c.PersonaId =p.Id 
inner join Empresas as empr on empr.Id=c.EmpresaId
left join CuotasSociales as cuot on cuot.Id=tpers.CuotaId
left join Localidad as loc on loc.Id=c.LocalidadId
left join Provincia as pr on pr.Id=c.ProvinciaId
inner join Prestamos as Pre on pre.ClienteId = c.id
left join Inversores as i on pre.InversorId=i.Id
LEFT JOIN TasasEfectivaAnual AS t1 ON Pre.TEA = t1.Id
LEFT JOIN TasasNominalAnual AS t2 ON Pre.TNA = t2.Id
LEFT JOIN CFTSinImpuesto AS t3 ON Pre.CFTSinImpuesto = t3.Id
LEFT JOIN CFTConImpuesto AS t4 ON Pre.CFTConImpuesto = t4.id 
LEFT JOIN TEM AS t5 ON i.TEMid=t5.Id 
where pre.id=@id</CommandText>
      </Query>
